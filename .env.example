# AI Hypnosis Platform - Environment Variables
# Copy this file to .env and fill in your values

# Cloudflare Configuration
CLOUDFLARE_ACCOUNT_ID=your_account_id_here
CLOUDFLARE_API_TOKEN=your_api_token_here

# AI API Keys (for production)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_AI_API_KEY=your_google_ai_key_here

# Database Configuration (auto-generated after wrangler d1 create)
D1_DATABASE_ID=your_database_id_here

# KV Namespace (auto-generated after wrangler kv:namespace create)
KV_NAMESPACE_ID=your_kv_namespace_id_here

# Analytics and Monitoring
SENTRY_DSN=your_sentry_dsn_here
ANALYTICS_ID=your_analytics_id_here

# Custom Domain (optional)
CUSTOM_DOMAIN=hypnosis.yourdomain.com

# Feature Flags
ENABLE_VOICE_COMMANDS=true
ENABLE_EMERGENCY_PROTOCOL=true
ENABLE_OFFENSIVE_MODE=true
ENABLE_PRACTICE_MODE=true
MAX_SESSION_DURATION=3600